{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "AquariumData2026": "Debug"
    }
  },
  "Mqtt": {
    "Host": "localhost",
    "Port": 1883,
    "ClientId": "aquarium-data-fetcher",
    "UseTls": false,
    "KeepAliveSeconds": 30,
    "ReconnectDelaySeconds": 5
  },
  "RabbitMq": {
    "Host": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest",
    "ExchangeName": "aquarium.telemetry",
    "RoutingKey": "aquarium.measurements",
    "Durable": true
  },
  "RegistryApi": {
    "BaseUrl": "https://registry.invalid",
    "AquariumsPath": "/api/aquariums",
    "TimeoutSeconds": 10
  },
  "Topics": {
    "UseHardCodedTopics": true,
    "TopicTemplate": "aquariums/{aquariumId}/measurements",
    "HardCodedTopics": [
      "aquariums/demo-reef/measurements",
      "aquariums/demo-freshwater/measurements"
    ]
  },
  "BinaryPayload": {
    "ExpectedVersion": 1
  }
}
